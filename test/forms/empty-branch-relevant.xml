<?xml version="1.0" encoding="utf-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Empty-branch-relevant</h:title>
    <model>
      <instance>
        <data id="Empty-branch-relevant" version="201801180931">
          <age/>
          <details>
            <name>Baby ...</name>
          </details>
          <meta>
            <instanceID/>
          </meta>
        </data>
      </instance>
      <bind nodeset="/data/age" type="int"/>
      <bind nodeset="/data/details/name" relevant=" /data/age  &lt;= 5" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/data/age">
      <label>What's your age?</label>
    </input>
    <group ref="/data/details">
      <input ref="/data/details/name">
        <label>What's your name?</label>
      </input>
    </group>
  </h:body>
</h:html>
